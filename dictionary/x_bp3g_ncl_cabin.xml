<?xml version="1.0" encoding="UTF-8"?>
<database>
    <element label="Cabin" max_length="40" name="x_bp3g_ncl_cabin" type="collection">
        <element choice="1" display="true" label="Category" max_length="40" name="category" type="choice">
            <choice>
                <element label="Inside" sequence="0" value="inside"/>
                <element label="OceanView" sequence="1" value="oceanview"/>
                <element label="Balcony" sequence="2" value="balcony"/>
                <element label="Mini-Suite" sequence="3" value="mini-suite"/>
                <element label="Suite" sequence="4" value="suite"/>
            </choice>
        </element>
        <element label="Count" max_length="40" name="count" type="integer"/>
        <element label="Guest Capacity" max_length="40" name="guest_capacity" type="integer"/>
        <element attributes="ignore_filter_on_new=true" default="javascript:global.getNextObjNumberPadded();" label="Number" max_length="40" name="number" type="string"/>
        <element choice="1" label="Occupancy Type" max_length="40" name="occupancy_type" type="choice">
            <choice>
                <element label="SINGLE" sequence="0" value="single"/>
                <element label="MULTIPLE" sequence="1" value="multiple"/>
            </choice>
        </element>
        <element label="Ship" max_length="32" name="ship" reference="x_bp3g_ncl_ship" type="reference"/>
        <index name="index">
            <element name="ship"/>
        </index>
    </element>
</database>
